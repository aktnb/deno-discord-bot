{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --allow-import main.ts",
    "test": "deno test --allow-read --allow-env --allow-net --allow-import",
    "lint": "deno lint",
    "generate": "deno run -A npm:drizzle-kit generate",
    "migrate": "deno run -A npm:drizzle-kit migrate"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@types/pg": "npm:@types/pg@^8.11.14",
    "discord.js": "npm:discord.js@^14.19.3",
    "drizzle-kit": "npm:drizzle-kit@^0.31.1",
    "drizzle-orm": "npm:drizzle-orm@^0.43.1",
    "pg": "npm:pg@^8.15.6"
  }
}
